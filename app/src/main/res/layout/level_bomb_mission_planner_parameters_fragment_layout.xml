<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:layout_centerVertical="true"
    android:layout_gravity="center"
    android:descendantFocusability="beforeDescendants"
    android:fillViewport="true"
    android:focusableInTouchMode="true">

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/level_release_parameters_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:paddingTop="10dp"
            android:text="@string/release_parameters"
            android:textAppearance="?android:attr/textAppearanceLarge"/>

        <TextView
            android:id="@+id/level_approach_course_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/level_release_parameters_text_view"
            android:labelFor="@+id/level_approach_course_edit_text"
            android:paddingTop="20dp"
            android:text="@string/approach_course"
            android:textAppearance="?android:attr/textAppearanceMedium"/>

        <TextView
            android:id="@+id/level_approach_course_considerations_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/level_approach_course_text_view"
            android:text="@string/approach_course_considerations"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@color/text_gray"
            android:textStyle="italic"/>

        <EditText
            android:id="@+id/level_approach_course_edit_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/level_approach_course_considerations_text_view"
            android:layout_alignBaseline="@+id/level_approach_course_text_view"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_toEndOf="@+id/space2"
            android:layout_toRightOf="@+id/space2"
            android:inputType="text"
            android:selectAllOnFocus="true"/>

        <TextView
            android:id="@+id/level_target_elevation_text_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/level_approach_course_considerations_text_view"
            android:paddingTop="20dp"
            android:text="@string/target_elevation"
            android:textAppearance="?android:attr/textAppearanceMedium"/>

        <TextView
            android:id="@+id/level_target_elevation_mean_sea_level"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/level_target_elevation_text_view"
            android:text="@string/mean_sea_level"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="@color/text_gray"
            android:textStyle="italic"/>

        <EditText
            android:id="@+id/level_target_elevation_edit_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@id/level_target_elevation_mean_sea_level"
            android:layout_alignBaseline="@+id/level_target_elevation_text_view"
            android:layout_alignBottom="@+id/level_target_elevation_text_view"
            android:layout_alignLeft="@+id/level_approach_course_edit_text"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignStart="@+id/level_approach_course_edit_text"
            android:layout_toEndOf="@+id/level_approach_course_text_view"
            android:layout_toRightOf="@+id/level_approach_course_text_view"
            android:inputType="textNoSuggestions"
            android:paddingTop="20dp"
            android:selectAllOnFocus="true"/>

        <TextView
