            android:layout_alignLeft="@+id/level_release_ktas_spinner"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignStart="@+id/level_release_ktas_spinner"
            android:layout_below="@id/level_select_ripple_quantity_spinner"
            android:inputType="textNoSuggestions"
            android:paddingTop="30dp"
            android:selectAllOnFocus="true"/>

        <Button
